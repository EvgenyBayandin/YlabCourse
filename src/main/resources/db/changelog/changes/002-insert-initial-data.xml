    <changeSet id="009-insert-admin-user" author="webdev" runOnChange="true">
        <insert tableName="users" schemaName="coworking_schema">
            <column name="username" value="admin"/>
            <column name="password" value="password"/>
            <column name="is_admin" valueBoolean="true"/>
        </insert>
    </changeSet>

    <changeSet id="010-insert-user" author="webdev">
        <insert tableName="users" schemaName="coworking_schema">
            <column name="username" value="username"/>
            <column name="password" value="password"/>
            <column name="is_admin" valueBoolean="false"/>
        </insert>
    </changeSet>

